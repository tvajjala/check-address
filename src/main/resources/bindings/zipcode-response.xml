<xml-bindings xmlns="http://www.eclipse.org/eclipselink/xsds/persistence/oxm"
              package-name="com.tvajjala.address.soap.model.response" xml-accessor-order="ALPHABETICAL">

    <xml-schema element-form-default="QUALIFIED">
        <xml-ns prefix="ws" namespace-uri="http://ws.cdyne.com"/>
    </xml-schema>

    <java-types>
        <java-type name="CityStateRes">
            <xml-root-element name="ReturnCityStateResponse"/>
            <xml-type namespace="http://ws.cdyne.com"/>
            <java-attributes>
                <xml-element java-attribute="county" xml-path="ReturnCityStateResult/County/text()"/>
                <xml-element java-attribute="state" xml-path="ReturnCityStateResult/StateAbbrev/text()"/>
                <xml-element java-attribute="city" xml-path="ReturnCityStateResult/City/text()"/>
                <xml-element java-attribute="zipCode" xml-path="ReturnCityStateResult/ZipCode/text()"/>
            </java-attributes>
        </java-type>
    </java-types>

</xml-bindings>